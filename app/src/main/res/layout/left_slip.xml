<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:layout_gravity="start"
    android:background="#FBF7E8"
    android:orientation="vertical">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#000000"
        android:padding="1dp"
        android:src="@drawable/fgo_logo1" />

    <TextView
        android:id="@+id/leftSlip_tev_rxjw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/textview_border"
        android:gravity="center"
        android:onClick="leftSlipSelect"
        android:text="茹西教王"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/leftSlip_tev_fgowiki"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/textview_border"
        android:gravity="center"
        android:onClick="leftSlipSelect"
        android:text="FOG中文wiki"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/leftSlip_tev_fireCalc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/textview_border"
        android:gravity="center"
        android:onClick="leftSlipSelect"
        android:text="狗粮计算"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/leftSlip_tev_servant"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/textview_border"
        android:gravity="center"
        android:onClick="leftSlipSelect"
        android:text="Servant"
        android:textSize="24sp" />

</LinearLayout>